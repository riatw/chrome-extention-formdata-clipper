{
	"manifest_version": 2,
	"name": "FormData Clipper",
	"description": "",
	"version": "1.0.1",
	"icons": {
		"128": "images/icon/icon_128.png"
	},
	"browser_action": {
		"default_icon": {
			"128": "images/icon/icon_128.png"
		},
		"default_title": "FormData Clipper",
		"default_popup": "popup.html"
	},
	"permissions": [
		"tabs"
	],
	"content_scripts": [{
		"matches": ["<all_urls>"],
		"js": [
			"js/jquery-2.1.1.min.js",
			"js/content.js"
		]
	}]
}